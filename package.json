{
    "name": "diana",
    "version": "0.0.1",
    "scripts": {
        "dev": "nodemon --watch src --ext ts,tsx --exec ts-node src/discord/bot.ts",
        "dev:debug": "nodemon --watch src --ext ts,tsx --exec \"node --inspect -r ts-node/register src/discord/bot.ts\"",
        "start": "node dist/discord/bot.js",
        "test": "jest",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint --fix --ext .js,.jsx .",
        "lint:check": "eslint --ext .js,.jsx .",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "backfill:matches": "ts-node src/scripts/backfillMissingMatches.ts",
        "precommit": "npm run lint && npm run format && npm run build && npm run test",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@eslint/js": "^9.25.0",
        "@types/pg": "^8.11.13",
        "axios": "^1.7.9",
        "discord.js": "^14.17.3",
        "dotenv": "^17.2.3",
        "eslint": "^9.25.0",
        "express": "^5.1.0",
        "globals": "^16.0.0",
        "jest": "^30.2.0",
        "node-cron": "^4.2.1",
        "pg": "^8.13.1",
        "prettier": "3.6.2",
        "twisted": "^1.61.5"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.8",
        "@types/express": "^5.0.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.0",
        "@types/node-cron": "^3.0.11",
        "babel-jest": "^30.2.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.5",
        "nodemon": "^3.1.9",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,yaml,yml}": [
            "prettier --write"
        ]
    }
}
